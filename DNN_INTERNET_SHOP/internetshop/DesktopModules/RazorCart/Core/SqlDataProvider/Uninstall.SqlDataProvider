/************************************************************/
/*****              SqlDataProvider                     *****/
/*****                                                  *****/
/*****                                                  *****/
/***** Note: To manually execute this script you must   *****/
/*****       perform a search and replace operation     *****/
/*****       for {databaseOwner} and {objectQualifier}  *****/
/*****                                                  *****/
/************************************************************/



/************************************************************/
/*****                Drop Tables                       *****/
/************************************************************/
DECLARE @rzcTable NVARCHAR(500)
DECLARE rzcTables CURSOR FOR
SELECT 'DROP TABLE {databaseOwner}[{objectQualifier}' + TABLE_NAME + ']'
	FROM INFORMATION_SCHEMA.TABLES
	WHERE TABLE_TYPE = 'BASE TABLE' AND TABLE_NAME IN (
		'RZC_Analytics'
		,'RZC_BookingDisabledDates'
		,'RZC_BookingDisabledTimes'
		,'RZC_BundleProducts'
		,'RZC_Category'
		,'RZC_CategoryProduct'
		,'RZC_CategoryQtyDiscounts'
		,'RZC_ClientCredentials'
		,'RZC_CouponProduct'
		,'RZC_Coupons'
		,'RZC_CouponVariant'
		,'RZC_Customer'
		,'RZC_CustomerWallet'
		,'RZC_Department'
		,'RZC_EmailCampaign'
		,'RZC_EmailTemplateSC'
		,'RZC_GiftCards'
		,'RZC_InvoiceTemplate'
		,'RZC_LicenseKey'
		,'RZC_Manufacturer'
		,'RZC_OrderDiscounts'
		,'RZC_OrderFulfillmentDetails'
		,'RZC_OrderFulfillments'
		,'RZC_OrderNotes'
		,'RZC_PayHist'
		,'RZC_PayHistImages'
		,'RZC_PaymentMethods'
		,'RZC_PaymentMethodsZones'
		,'RZC_PriceClass'
		,'RZC_ProductCustomFields'
		,'RZC_ProductDiscountsByRole'
		,'RZC_ProductEmail'
		,'RZC_ProductDownloadHistory'
		,'RZC_ProductImages'
		,'RZC_ProductLocalization'
		,'RZC_ProductManageRole'
		,'RZC_ProductPackage'
		,'RZC_ProductQtyDiscounts'
		,'RZC_ProductReviews'
		,'RZC_Products'
		,'RZC_Products2'
		,'RZC_ProductTemplate'
		,'RZC_ProductVariant'
		,'RZC_ProductVariantGroup'
		,'RZC_ProductVariantInventory'
		,'RZC_ProductVariantInvLinker'
		,'RZC_ProductVariantRole'
		,'RZC_ProductVisibilityRole'
		,'RZC_RelatedProducts'
		,'RZC_SavedCart'
		,'RZC_SavedCartDetail'
		,'RZC_SavedCartDetailVariant'
		,'RZC_SavedSearches'
		,'RZC_SearchKeys'
		,'RZC_SessionState'
		,'RZC_ShipCategories'
		,'RZC_ShipFree'
		,'RZC_ShipMethods'
		,'RZC_ShippingByAmount'
		,'RZC_ShippingByQuantity'
		,'RZC_ShippingByWeight'
		,'RZC_ShippingByZipCode'
		,'RZC_ShipZone'
		,'RZC_ShoppingCart'
		,'RZC_ShoppingCartVariant'
		,'RZC_StoreConfig'
		,'RZC_StoreOrderDetails'
		,'RZC_StoreOrderDetailVariant'
		,'RZC_StoreOrders'
		,'RZC_StorePermissions'
		,'RZC_Stores'
		,'RZC_SubscriptionSC'
		,'RZC_Tax'
		,'RZC_TaxStates'
		,'RZC_VariantConditionalGroup'
		,'RZC_VendorBooking'
	)
OPEN rzcTables WHILE 1 = 1
BEGIN
    FETCH rzcTables INTO @rzcTable
    IF @@FETCH_STATUS != 0 BREAK
    EXEC (@rzcTable)
END
CLOSE rzcTables;
DEALLOCATE rzcTables



/************************************************************/
/*****                Drop Sprocs                       *****/
/************************************************************/
